{
  "name": "redux-dutiful-thunk",
  "version": "0.0.0",
  "description": "Use thunk action without breaking Redux types",
  "author": "ryym",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryym/redux-dutiful-thunk.git"
  },
  "keywords": [
    "redux",
    "redux-thunk"
  ],
  "main": "dist/index.js",
  "files": [
    "src",
    "dist"
  ],
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test": "TS_NODE_CACHE=0 mocha --require ts-node/register/type-check 'src/*_test.ts'",
    "test:w": "npm run test -- --watch --watch-extensions ts",
    "lint": "tslint -p . -t verbose 'src/**/*.ts'",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test"
  },
  "dependencies": {
    "redux": "4.0.0-beta.2"
  },
  "devDependencies": {
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.2",
    "mocha": "^5.0.5",
    "prettier": "^1.11.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  }
}
